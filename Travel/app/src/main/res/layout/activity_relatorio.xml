<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background">


    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="29dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="141dp"
        android:text="Alimentação"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView"
        android:layout_alignStart="@+id/textView"
        android:layout_below="@+id/textView"
        android:layout_marginTop="47dp"
        android:text="Transporte"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView2"
        android:layout_alignStart="@+id/textView2"
        android:layout_below="@+id/textView2"
        android:layout_marginTop="51dp"
        android:text="Lazer"
        android:textColor="#ffffff"
        android:textColorLink="#ffffff" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView3"
        android:layout_alignStart="@+id/textView3"
        android:layout_below="@+id/textView3"
        android:layout_marginTop="44dp"
        android:text="Extra"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView4"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/textView4"
        android:layout_marginBottom="50dp"
        android:text="TOTAL"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/valorAlimentacao"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView"
        android:layout_alignBottom="@+id/textView"
        android:layout_marginLeft="65dp"
        android:layout_marginStart="65dp"
        android:layout_toEndOf="@+id/textView"
        android:layout_toRightOf="@+id/textView"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/valorAlimentacaoMaximo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/valorAlimentacao"
        android:layout_marginLeft="19dp"
        android:layout_marginStart="19dp"
        android:layout_toEndOf="@+id/valorAlimentacao"
        android:layout_toRightOf="@+id/valorAlimentacao"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/valorTransporte"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/valorAlimentacao"
        android:layout_alignStart="@+id/valorAlimentacao"
        android:layout_alignTop="@+id/textView2"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/valorTransporteMaximo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView3"
        android:layout_alignEnd="@+id/valorAlimentacaoMaximo"
        android:layout_alignRight="@+id/valorAlimentacaoMaximo"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/valorLazer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/valorTransporte"
        android:layout_alignRight="@+id/valorTransporte"
        android:layout_alignTop="@+id/textView3"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/valorLazerMaximo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/valorTransporteMaximo"
        android:layout_alignStart="@+id/valorTransporteMaximo"
        android:layout_alignTop="@+id/valorLazer"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/valorOutros"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView4"
        android:layout_alignLeft="@+id/valorLazer"
        android:layout_alignStart="@+id/valorLazer"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/valorTotal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/valorOutros"
        android:layout_alignStart="@+id/valorOutros"
        android:layout_alignTop="@+id/textView5"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/valorOutrosMaximo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/valorOutros"
        android:layout_toEndOf="@+id/valorLazerMaximo"
        android:layout_toRightOf="@+id/valorLazerMaximo" />

    <TextView
        android:id="@+id/valorDisponivel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/valorTotal"
        android:layout_toEndOf="@+id/valorOutrosMaximo"
        android:layout_toRightOf="@+id/valorOutrosMaximo" />
</RelativeLayout>
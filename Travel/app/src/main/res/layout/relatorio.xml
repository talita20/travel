<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">


    <TextView
        android:id="@+id/alimentacaoView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="23dp"
        android:layout_marginStart="23dp"
        android:layout_marginTop="67dp"
        android:text="Alimentação" />

    <TextView
        android:id="@+id/transporteView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/alimentacaoView"
        android:layout_alignStart="@+id/alimentacaoView"
        android:layout_below="@+id/alimentacaoView"
        android:layout_marginTop="33dp"
        android:text="Transporte" />

    <TextView
        android:id="@+id/lazerView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/transporteView"
        android:layout_alignStart="@+id/transporteView"
        android:layout_below="@+id/transporteView"
        android:layout_marginTop="34dp"
        android:text="Lazer" />

    <TextView
        android:id="@+id/outrosView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/lazerView"
        android:layout_alignStart="@+id/lazerView"
        android:layout_below="@+id/lazerView"
        android:layout_marginTop="34dp"
        android:text="Outros" />

    <TextView
        android:id="@+id/totalView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/outrosView"
        android:layout_alignStart="@+id/outrosView"
        android:layout_below="@+id/outrosView"
        android:layout_marginTop="92dp"
        android:text="TOTAL" />

    <TextView
        android:id="@+id/alimentacaoTotalView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/alimentacaoView"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginEnd="82dp"
        android:layout_marginRight="82dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/alimentacaoDisponivelView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/alimentacaoTotalView"
        android:layout_marginEnd="21dp"
        android:layout_marginRight="21dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/transporteTotalView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/transporteView"
        android:layout_toLeftOf="@+id/alimentacaoDisponivelView"
        android:layout_toStartOf="@+id/alimentacaoDisponivelView"
        android:text="TextView" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/alimentacaoDisponivelView"
        android:layout_alignStart="@+id/alimentacaoDisponivelView"
        android:layout_alignTop="@+id/transporteTotalView"
        android:text="TextView" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/alimentacaoTotalView"
        android:layout_alignRight="@+id/alimentacaoTotalView"
        android:layout_alignTop="@+id/lazerView"
        android:text="TextView" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView9"
        android:layout_alignStart="@+id/textView9"
        android:layout_alignTop="@+id/textView10"
        android:text="TextView" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/totalView"
        android:layout_toLeftOf="@+id/textView11"
        android:layout_toStartOf="@+id/textView11"
        android:text="TextView" />

    <TextView
        android:id="@+id/outrosDisponivelView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView11"
        android:layout_alignStart="@+id/textView11"
        android:layout_alignTop="@+id/textView12"
        android:text="TextView" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/totalView"
        android:layout_alignEnd="@+id/textView10"
        android:layout_alignRight="@+id/textView10"
        android:text="TextView" />

    <TextView
        android:id="@+id/totalDisponivelView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/outrosDisponivelView"
        android:layout_alignStart="@+id/outrosDisponivelView"
        android:layout_alignTop="@+id/textView17"
        android:text="TextView" />

</RelativeLayout>